  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SpikeVlg/sem_ver_test.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SpikeVlg/sem_ver_test/issues"
